{
	"openapi": "3.0.0",
	"info": {
		"title": "Google Maps Reviews API",
		"version": "1.0.0",
		"description": "API for scraping reviews from Google Maps"
	},
	"servers": [
		{
			"url": "/api/v1",
			"description": "API v1"
		}
	],
	"components": {
		"schemas": {},
		"parameters": {}
	},
	"paths": {
		"/restaurants": {
			"get": {
				"tags": ["restaurants"],
				"summary": "Get restaurants matching query",
				"description": "Returns a list of restaurants matching the search query",
				"parameters": [
					{
						"schema": {
							"type": "string",
							"description": "identifier of the restaurant"
						},
						"required": true,
						"description": "identifier of the restaurant",
						"name": "query",
						"in": "query"
					}
				],
				"responses": {
					"200": {
						"description": "Restaurants found successfully",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"success": {
											"type": "boolean"
										},
										"restaurants": {
											"type": "array",
											"items": {
												"type": "object",
												"properties": {
													"id": {
														"type": "string",
														"nullable": true
													},
													"name": {
														"type": "string",
														"nullable": true
													},
													"rating": {
														"type": "number",
														"nullable": true
													},
													"reviewCount": {
														"type": "number",
														"nullable": true
													},
													"categories": {
														"type": "array",
														"nullable": true,
														"items": {
															"type": "string"
														}
													},
													"address": {
														"type": "string",
														"nullable": true
													},
													"priceLevel": {
														"type": "string",
														"nullable": true
													},
													"openNow": {
														"type": "boolean"
													},
													"phone": {
														"type": "string",
														"nullable": true
													},
													"website": {
														"type": "string",
														"nullable": true
													},
													"photos": {
														"type": "array",
														"items": {
															"type": "string"
														}
													},
													"accessibility": {
														"type": "array",
														"nullable": true,
														"items": {
															"type": "string"
														}
													}
												},
												"required": [
													"id",
													"name",
													"rating",
													"reviewCount",
													"categories",
													"address",
													"priceLevel",
													"openNow",
													"phone",
													"website",
													"photos",
													"accessibility"
												]
											}
										},
										"error": {
											"type": "string"
										}
									},
									"required": ["success", "restaurants"]
								}
							}
						}
					},
					"400": {
						"description": "Bad request or no restaurants found",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"success": {
											"type": "boolean"
										},
										"error": {
											"type": "string"
										}
									},
									"required": ["success", "error"]
								}
							}
						}
					},
					"500": {
						"description": "Server error",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"success": {
											"type": "boolean"
										},
										"error": {
											"type": "string"
										}
									},
									"required": ["success", "error"]
								}
							}
						}
					}
				}
			}
		},
		"/restaurants/dishes": {
			"get": {
				"tags": ["restaurants"],
				"summary": "Find popular dishes from restaurant reviews",
				"description": "Analyzes restaurant reviews to identify the most popular dishes mentioned",
				"parameters": [
					{
						"schema": {
							"type": "string",
							"description": "identifier of the restaurant"
						},
						"required": true,
						"description": "identifier of the restaurant",
						"name": "query",
						"in": "query"
					}
				],
				"responses": {
					"200": {
						"description": "Popular dishes analysis completed successfully",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"success": {
											"type": "boolean"
										},
										"totalReviewsAnalyzed": {
											"type": "number"
										},
										"popularDishes": {
											"type": "array",
											"items": {
												"type": "object",
												"properties": {
													"name": {
														"type": "string",
														"description": "Name of the dish"
													},
													"mentions": {
														"type": "number",
														"description": "Number of mentions in reviews"
													},
													"rating": {
														"type": "number",
														"description": "Average rating for this dish (0-5)"
													},
													"sentimentScore": {
														"type": "number",
														"minimum": -1,
														"maximum": 1,
														"description": "Sentiment score for this dish"
													},
													"reviewExcerpts": {
														"type": "array",
														"items": {
															"type": "string"
														},
														"description": "Excerpts from reviews mentioning this dish"
													}
												},
												"required": ["name", "mentions", "rating", "sentimentScore", "reviewExcerpts"]
											}
										}
									},
									"required": ["success", "totalReviewsAnalyzed", "popularDishes"]
								}
							}
						}
					},
					"400": {
						"description": "Bad request or analysis error",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"success": {
											"type": "boolean"
										},
										"error": {
											"type": "string"
										}
									},
									"required": ["success", "error"]
								}
							}
						}
					},
					"500": {
						"description": "Server error",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"success": {
											"type": "boolean"
										},
										"error": {
											"type": "string"
										}
									},
									"required": ["success", "error"]
								}
							}
						}
					}
				}
			}
		}
	}
}
